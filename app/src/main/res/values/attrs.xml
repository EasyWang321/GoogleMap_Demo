<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="ms_panel_height" format="dimension" />
        <attr name="ms_shadow_height" format="dimension" />
        <attr name="ms_parallax_offset" format="dimension" />
        <attr name="ms_fade_color" format="color" />
        <attr name="ms_fling_velocity" format="integer" />
        <attr name="ms_drag_view" format="reference" />
        <attr name="ms_scrollable_view" format="reference" />
        <attr name="ms_overlay" format="boolean" />
        <attr name="ms_clip_panel" format="boolean" />
        <attr name="ms_anchor_point" format="float" />
        <attr name="ms_initial_state" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
        <attr name="ms_scroll_interpolator" format="reference" />
    </declare-styleable>

</resources>
